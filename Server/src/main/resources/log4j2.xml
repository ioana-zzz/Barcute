<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="info">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern=" %class{36} %L %M - %msg%xEx%n"/>
        </Console>
        <File name="log" fileName="C:\Users\ioana\Desktop\mpp\Barcuta\Server\src\main\JocMPPBarca.log" append="true">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %-5level %class{36} %L %M - %msg%xEx%n"/>
        </File>
    </Appenders>
    <Loggers>
        <Root level="trace">
            <AppenderRef ref="Console"/>
            <AppenderRef ref="log"/>
        </Root>
    </Loggers>
</Configuration>
